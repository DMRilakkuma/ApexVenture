<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apxvt.auth.mapper.UserLoginMapper">
    <select id="selectUseByName" resultType="com.apxvt.api.domain.SysUser">
        select *
        from sys_user
        where user_name = #{username}
    </select>
</mapper>